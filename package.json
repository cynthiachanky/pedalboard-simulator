{
  "name": "pedalboard-simulator",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "prebuild": "pip3 install -r requirements.txt && python3 prebuild.py && cp README.md app/about/page.mdx",
    "flask-dev": "pip3 install -r requirements.txt && python3 -m flask --app api/index run --debug",
    "next-dev": "next dev",
    "dev": "pnpm prebuild && concurrently \"pnpm run next-dev\" \"pnpm run flask-dev\"",
    "build": "pnpm prebuild && next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@formkit/drag-and-drop": "^0.1.6",
    "@mdx-js/loader": "^3.0.1",
    "@mdx-js/react": "^3.0.1",
    "@next/mdx": "^14.2.5",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-tooltip": "^1.1.2",
    "@types/mdx": "^2.0.13",
    "@types/node": "22.0.3",
    "class-variance-authority": "0.7.0",
    "clsx": "2.1.1",
    "concurrently": "8.2.2",
    "jotai": "2.9.1",
    "lucide-react": "0.424.0",
    "next": "14.2.5",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "tailwind-merge": "2.4.0",
    "tailwindcss-animate": "1.0.7"
  },
  "devDependencies": {
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.19",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.5",
    "eslint-config-prettier": "9.1.0",
    "postcss": "8.4.40",
    "prettier": "3.3.3",
    "tailwindcss": "3.4.7",
    "typescript": "5.5.4"
  }
}
